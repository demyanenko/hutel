{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}\\server\\bin\\Debug\\netcoreapp1.0\\server.dll",
            "args": [],
            "cwd": "${workspaceRoot}\\server",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "HUTEL_USE_GOOGLE_AUTH": "1",
                "GOOGLE_PROJECT_ID": "human-telemetry",
                "GOOGLE_CLIENT_SECRET": "Rr-By8-VB3WWtspT-nIrFha5",
                "GOOGLE_CLIENT_ID": "969040452809-n48m7rhvqshripug67rm11bcsjtctcm0.apps.googleusercontent.com",
                "HUTEL_USE_GOOGLE_DRIVE": "1"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command.pickProcess}"
        }
    ]
}